<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/parent_relative_result"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@null">

    <ExpandableListView
        android:id="@+id/expandable_list"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@+id/et_result_buttom"
        android:divider="@android:color/white"
        android:dividerHeight="2dp"
        tools:listheader="@layout/list_group"
        tools:listitem="@layout/list_item">

    </ExpandableListView>

    <android.support.v7.widget.AppCompatButton
        android:id="@+id/et_result_buttom"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="50dp"
        android:layout_marginEnd="@dimen/activity_horizontal_margin"
        android:layout_marginStart="@dimen/activity_horizontal_margin"
        android:background="@drawable/drawable_background_login_button"
        android:text="@string/print"
        android:textColor="@android:color/white" />
</RelativeLayout>